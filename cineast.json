{
	"database": {
		"host": "127.0.0.1",
		"port": 5890,
		"plaintext": true
	},
	
	"retriever": {
		"threadPoolSize": 2,
		"maxResults": 100,
		"resultsPerModule": 50
	},
	
	"decoder": {
		"maxFrameWidth": 640,
		"maxFrameHeight": 480,
		"decoder": "FFMPEG"
	},
	
	"extractor": {
		"threadPoolSize": 2
	},
	
	"imagecache": {
		"softMemoryLimit": 2048,
		"hardMemoryLimit": 1024
	},
	
	"api": {
		"jsonApiPort": 12345,
		"enableCLI": true
	},
	"visualization": {
		"cacheEnabled": false,
		"cachePath": "cache/art/"
	}

}